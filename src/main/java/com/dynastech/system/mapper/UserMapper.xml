	<!-- 只查本级 -->
	<select id="selectUsersByOrgId" parameterType="map" resultMap="ResultMapWithRole">
		select 
		<include refid="Base_Column_List" />,<include refid="Role_Base_Column_List" />
		from RS_User u 
		INNER JOIN RS_OrganizationUser orgu on u.Id=orgu.UserId 
		LEFT JOIN RS_RoleUser ru ON u.id= ru.UserId  and ru.OrganizationId=orgu.OrganizationId
		INNER JOIN RS_Role R ON R.ID=ru.RoleId
		where  u.IsDeleted=0 and r.IsDeleted=0
		and orgu.Organizationid=#{orgId}
		<if test="str !=null and str !='' ">
			and (u.accountname like concat('%',#{str},'%') or
			u.DisplayName like
			concat('%',#{str},'%') )
		</if>
	</select>
	
	<select id="findLeadersByOrgId" parameterType="map" resultMap="BaseResultMap">
		select distinct
		<include refid="Base_Column_List" />
		from rs_user u , RS_OrganizationUser ou ,RS_RoleUser ru ,RS_Role r,RS_OrganizationDepth od
		where  u.id=ou.UserId  and u.id=ru.UserId and ru.RoleId=r.id
		and ou.OrganizationId=ru.OrganizationId
		and ou.OrganizationId=od.ChildId
		and r.name=#{roleName}
		and u.isdeleted =0
		and r.IsDeleted=0
		and od.ParentId in 
		<foreach close=")" collection="orgIds" item="id" open="(" separator=",">
			#{id,jdbcType=VARCHAR}
		</foreach>
	</select>
	
	<select id="findUsersByRoleName" parameterType="map" resultMap="BaseResultMap">
		select distinct <include refid="Base_Column_List" />
			 from RS_User u ,RS_RoleUser ru ,RS_Role r
		where u.Id=ru.UserId
		and r.Id=ru.RoleId
		and r.Name=#{roleName}
		and u.IsDeleted=0
