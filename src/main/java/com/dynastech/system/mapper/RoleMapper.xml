 <select id="selectRolesByOrgId" parameterType="map" resultMap="BaseResultMap">
	select
	<include refid="Base_Column_List" />,org.DisplayName as organizationName
	
	from RS_role r,RS_Organization org,RS_OrganizationDepth orgd
	where r.OrganizationId=org.id and org.id=orgd.ChildId 
	and orgd.ParentId=#{orgId,jdbcType=VARCHAR}
	and r.IsDeleted=0
	<if test="str !=null and str !='' ">
		and r.name like concat('%',#{str},'%')
	</if>
</select>

<select id="selectRolesByUserId" parameterType="map" resultMap="BaseResultMap">
	select
	<include refid="Base_Column_List" />,ru.OrganizationId as orgId
	from rs_role r ,RS_RoleUser ru 
		where r.id=ru.RoleId 
	and ru.UserId=#{userId}
	and r.IsDeleted=0
</select>

<select id="selectRolesByUserIdAndOrgId" parameterType="map" resultMap="BaseResultMap">
	select
	<include refid="Base_Column_List" />
	from rs_role r ,RS_RoleUser ru 
		where r.id=ru.RoleId 
	and ru.UserId=#{userId}
	and ru.OrganizationId=#{orgId}
	and r.IsDeleted=0
</select>

